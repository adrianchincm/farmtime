<% tokens.each do |token| %>
  <% case token
		when "osmosis" %>
  <%= image_tag 'osmosis.svg', width: '30', height: '30', class: "pr-2" %>
  <% when "usd-coin" %>
  <%= image_tag 'usdc.svg', width: '30', height: '30', class: "pr-2" %>
  <% when "terra-luna" %>
  <%= image_tag 'luna.svg', width: '30', height: '30', class: "pr-2" %>
  <% when "cosmos" %>
  <%= image_tag 'atom.svg', width: '30', height: '30', class: "pr-2" %>
  <% when "terrausd" %>
  <%= image_tag 'ust.png', width: '30', height: '30, class: "pr-2"' %>
  <% when "bitcoin" %>
  <%= image_tag 'bitcoin.svg', width: '30', height: '30', class: "pr-2" %>
  <% when "ethereum", "wrapped-steth", "binance-eth" %>
  <%= image_tag 'ethereum.svg', width: '24', height: '24', class: "pr-2" %>
  <% when "fantom" %>
  <%= image_tag 'fantom.svg', width: '30', height: '30', class: "pr-2" %>
  <% when "tether" %>
  <%= image_tag 'usdt.svg', width: '30', height: '30', class: "pr-2" %>  
<% end %>
<% end %>
